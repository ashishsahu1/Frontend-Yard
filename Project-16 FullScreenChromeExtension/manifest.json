{
    "manifest_version":2,
    "name":"Go FullScreen",
    "version":"1.0.0",
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js; object-src 'self'",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["Content.js"]
        }
    ],
    
    "browser_action":{
        "default_popup":"index.html",
        "default_title":"fullscreen"
    }
}